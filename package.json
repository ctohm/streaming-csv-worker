{
  "name": "@ctohm/streaming-csv-worker",
  "description": "Streaming parser of incoming CSV. It can return a stream of JSON or a regular JS array",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:ctohm/streaming-csv-worker.git"
  },
  "types": "dist/main.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "types": "tsc --project .  && cp src/main.d.ts dist/",
    "build": "esbuild src/index.ts --sourcemap=inline --bundle  --outdir=dist",
    "deploy": "rm dist/* &&   yarn types && yarn build && yarn publish"
  },
  "author": {
    "email": "ffigueroa@ctohm.com",
    "name": "Felipe Figueroa"
  },
  "license": "UNLICENSED",
  "version": "0.7.0",
  "devDependencies": {
    "@cloudflare/workers-types": "^2.2.2",
    "@types/csv-parse": "^1.2.2",
    "csv-parse": "^4.16.0",
    "esbuild": "^0.12.17",
    "miniflare": "^1.3"
  }
}
