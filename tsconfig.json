{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "ES2020",
    "module": "CommonJS",
    "lib": [
      "ES2020",
      "WebWorker"
    ],
    "isolatedModules": true,
    "resolveJsonModule": true,
    "alwaysStrict": true,
    "strict": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "types": [
      "@cloudflare/workers-types",
      "@types/node"
    ],
    "rootDir": "./src",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "declaration": true,
    "emitDeclarationOnly": false,
   
    "declarationMap": true
  },
  "include": [
    "./src/*.d.ts",
    "./src/*.ts"
  ],
  "exclude": [
    "node_modules/",
    "dist/"
  ]
}